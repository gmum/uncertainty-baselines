{
    "seed": 1337,
    "dataset": "cifar10",
    "validation_percent": 0.1,
    
    "output_dir": "outdir",
    "model_file": "outdir/model.ckpt-250",

    "optimizer": "sgd",
    "learning_rate": 0.1,
    "lr_scheduler": "piecewise_linear",
    "momentum": 0.9,
    "beta_1": 0.9,
    "epsilon": 0.001,
    "weight_decay": 0.0001,
    "augment_train": true,

    "use_early_stopping": true,
    "save_checkpoints": true,
    "use_tensorboard": false,    
    
    "activation": "relu",
    "certainty_variant": "partial",
    "model_variant": "vanilla",
    "logit_variant": "affine",
    
    "epochs": 250,    
    "batch_size": 128,
    "eval_batch_size": 100,
    "eval_frequency": 100,
    
    "num_bins": 15,

    "tune_hyperparams": false,    
    "tuner_dir": "tune_dir",
    "tuner_subdir": "subdir1",
    "tune_by": "acc",
    "tune_num_trials": 100,
    "tune_batch_size": 512,
    "tune_epochs":250,
    
    "calibration_variant": "nonlin",
    "which_output": "certs",
    "which_dataset": "val",
    "cal_dict": {"epochs": 10000,
                  "bins": 1000,
                  "basis_type": "uniform",
                  "basis_params": [-20, 20, 100]}
}